<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.songsearchengine.LogInController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Pane layoutX="2.0" layoutY="156.0" prefHeight="400.0" prefWidth="980.0" styleClass="pane">
            <children>
                <Label layoutX="21.0" layoutY="98.0" text="Dont have an account?" textFill="#ecebeb">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Label>
                <Label layoutX="21.0" layoutY="130.0" prefHeight="64.0" prefWidth="202.0" text="Click &quot;Sign Up&quot; to Make an Account" textFill="#ecebeb" wrapText="true">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <Button layoutX="21.0" layoutY="200.0" mnemonicParsing="false" onAction="#open_signup" prefHeight="32.0" prefWidth="93.0" styleClass="trans-btn-white-boarder" text="Sign Up" />
                <Label layoutX="759.0" layoutY="53.0" text="Have an account?" textFill="#ecebeb">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Label>
                <Label layoutX="759.0" layoutY="83.0" prefHeight="64.0" prefWidth="202.0" text="Click &quot;Sign In&quot; to Login To Account" textFill="#ecebeb" wrapText="true">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <Button layoutX="759.0" layoutY="162.0" mnemonicParsing="false" onAction="#open_signin" prefHeight="32.0" prefWidth="93.0" styleClass="trans-btn-white-boarder" text="Sign In" />
                <VBox id="vbox" fx:id="vbox" layoutX="21.0" layoutY="-69.0" prefHeight="480.0" prefWidth="480.0" styleClass="white-pane" stylesheets="@styles.css" />
            </children>
        </Pane>
    </children>

</AnchorPane>
